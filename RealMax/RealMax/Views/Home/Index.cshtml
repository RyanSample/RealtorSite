
@{    
    ViewBag.Title = "Home Page";
}

@section head{
    <link href="@Url.Content("~/Content/css/home/main.css")" rel="stylesheet" type="text/css" />
}

<div class="container">
    <div class="row">
        <div class="col-md-12 text-center">

            <div class="row">
                @{
                    Html.RenderAction("ListingPartial");
                }
                <h3><a href="@Url.Action("Index", "Listing")" style="color: inherit; text-decoration: none;">Check out our other listings!</a></h3>
            </div>
            <hr />
            <div class="row">
                <h3>Realtors of the month</h3>
                @{
                    Html.RenderAction("RealtorPartial");
                }
                <h3><a href="@Url.Action("Index", "Realtor")" style="color: inherit; text-decoration: none;">Meet the rest of our winning team!</a></h3>
            </div>

        </div>
    </div>
</div>