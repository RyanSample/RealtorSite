@model IEnumerable<RealMax.Models.Realtor>

@{
    ViewBag.Title = "Index";
}
@section head {
    @*<link rel="stylesheet" href="@Url.Content("~/Content/css/searchStyleSheets/searchIndexStyleSheet.css")" type="text/css" />*@
    @Styles.Render("~/Content/searchStyleSheets");
    }


<h2>Index</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<div class="row">
    <form class="navbar-form navbar-left" role="search" method="get" action="@Url.Action("Index", "Realtor")">
        <div class="form-group">
            <input type="text" class="form-control" placeholder="Search" name="id">
        </div>
        <button type="submit" value="submit" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
    </form>
</div>
@*<div class="row">*@
    @foreach (var item in Model)
    {

        <div class="col-md-4">
            <div class="thumbnail">
                <img src="..." alt="..." />
                <div class="caption">
                    <h3>
                        @Html.DisplayFor(modelItem => item.FirstName)

                        @Html.DisplayFor(modelItem => item.LastName)
                    </h3>
                    <p>
                        @Html.DisplayFor(modelItem => item.Email)

                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                    </p>
                    <p>
                        @Html.DisplayFor(modelItem => item.Bio)
                    </p>
                </div>
            </div>
        </div>
        @*<td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ID })
            </td>*@

    }
<div class="row"></div>
@*</div>*@


